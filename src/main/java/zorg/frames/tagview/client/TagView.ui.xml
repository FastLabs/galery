<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="tagStyle" type="zorg.frames.tagview.client.TagView.Style">
    </ui:with>
    <div class="{tagStyle.tagContainer}">
        <span class="{tagStyle.tagElement}">
            <span class="{tagStyle.internalTagElement}">
                <span class="{tagStyle.prefixIcon}"></span>
                <span class="{tagStyle.tagText}">test</span>
                <span class="{tagStyle.postfixIcon}"></span>
            </span>
        </span>
        <span class="{tagStyle.actionContainer}">
            <span class="{tagStyle.addLabelContainer}">
                <span class="{tagStyle.addButton}"></span>
                <span class="{tagStyle.addLabel}">Add more</span>
            </span>
            <input type="text" class="{tagStyle.addInput}"></input>
        </span>
    </div>
</ui:UiBinder>